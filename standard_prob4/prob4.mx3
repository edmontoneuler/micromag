Nx := 200
Ny := 50
setgridsize(Nx, Ny, 1)
setcellsize(500e-9/Nx, 125e-9/Ny, 3e-9)

Msat = 800e3
Aex = 13e-12
alpha = 0.02

m = uniform(1, .1, 0)
relax()

tableautosave(5e-12)

// B_ext = vector(-24.6E-3, 4.3E-3, 0) // Field 1
B_ext = vector(-35.5E-3, -6.3E-3, 0) // Field 2 

mx := m.comp(0)
runWhile(mx.average() >0)
save(m)
run(1e-9)
